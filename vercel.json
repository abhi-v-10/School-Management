{
  "version": 2,
  "builds": [
    { "src": "managementProject/asgi.py", "use": "@vercel/python", "config": { "runtime": "python3.11", "maxLambdaSize": "80mb", "buildCommand": "bash build_files.sh" } }
  ],
  "routes": [
    { "src": "/ws/(.*)", "dest": "/managementProject/asgi.py" },
    { "src": "/(.*)", "dest": "/managementProject/asgi.py" }
  ]
}
