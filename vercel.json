{
  "version": 2,
  "buildCommand": "bash build_files.sh",
  "builds": [
    { "src": "managementProject/wsgi.py", "use": "@vercel/python", "config": { "runtime": "python3.11", "maxLambdaSize": "50mb" } }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/staticfiles/$1" },
    { "src": "/(.*)", "dest": "/managementProject/wsgi.py" }
  ]
}
